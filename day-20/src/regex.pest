regex       =  { SOI ~ "^" ~ seq ~ "$" ~ EOI }
  seq       =  { (side | group)+ }
    side    = _{ east | west | north | south }
      east  =  { "E" }
      west  =  { "W" }
      north =  { "N" }
      south =  { "S" }
    group   =  { "(" ~ seq ~ ("|" ~ seq)* ~ bar? ~ ")" }
      bar   =  { "|" }
